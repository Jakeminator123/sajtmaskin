{
  "$schema": "https://unimported.vercel.app/schema/unimportedrc.schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/components/**/*.{ts,tsx}",
    "src/lib/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.*",
    "**/*.spec.*",
    ".next/**",
    "node_modules/**",
    "coverage/**",
    "src/middleware.ts",
    "**/middleware.ts"
  ],
  "ignoreUnimported": ["src/middleware.ts"],
  "aliases": {
    "@": "./src"
  }
}
